INSTDIR=../inst

all: wbo

wbo:
	cd open_wbo && $(MAKE) rs
	mkdir -p $(INSTDIR)/bin
	cp -f open_wbo/open-wbo_static$(EXEEXT) $(INSTDIR)/bin/open-wbo_static$(EXEEXT)

clean: clean-src clean-wbo

clean-src:
	rm -f *.o *.so *.rds
	rm -r $(INSTDIR)

clean-wbo:
	cd open_wbo && $(MAKE) allclean

.PHONY: all wbo clean clean-src clean-wbo
